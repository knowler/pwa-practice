{
	"name": "Practice PWA",
	"start_url": "/",
	"display": "standalone",
	"orientation": "landscape",
	"theme_color": "#111",
	"background_color": "#111",
	"launch_handler": {
		"client_mode": "navigate-existing"
	},
	"icons": [
		{
			"src": "icons/512.png",
			"type": "image/png",
			"sizes": "512x512"
		}
	],
	"share_target": {
		"action": "/share",
		"method": "POST",
		"enctype": "multipart/form-data",
		"params": {
			"url": "link"
		}
	},
	"shortcuts": [
		{
			"name": "Bookmark",
			"url": "/share"
		}
	]
}
